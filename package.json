{
  "name": "quiz-app-frontend",
  "version": "3.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.3.21",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@craco/craco": "^7.1.0",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "^11.0.0",
    "@hookform/resolvers": "^2.8.3",
    "@types/react": "^17.0.15",
    "@types/react-router-dom": "^5.3.3",
    "ajv": "^8.12.0",
    "axios": "1.6.0",
    "chakra-ui-autocomplete": "^1.4.5",
    "cross-fetch": "^3.1.5",
    "downshift": "6.0.6",
    "file-loader": "^6.2.0",
    "framer-motion": "^4.1.7",
    "gapi-script": "^1.2.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.12.6",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^18.3.1",
    "react-confetti": "^6.1.0",
    "react-countdown-circle-timer": "^3.0.9",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^3.1.4",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^7.27.1",
    "react-icons": "^4.4.0",
    "react-loader-spinner": "^5.3.4",
    "react-papaparse": "^4.1.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.1",
    "react-searchable-dropdown": "^2.0.3",
    "recharts": "^2.1.14",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5",
    "type-fest": "^2.5.1",
    "typescript": "^4.4.4",
    "zod": "^3.11.6"
  },
  "scripts": {
    "start": "HTTPS=true REACT_APP_API_MOCKING=false craco start",
    "clean": "rm -rf build/* ",
    "build": "craco build",
    "test": "jest --forceExit --maxWorkers=2 --silent --testTimeout=180000",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .prettierignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check-format": "npm run prettier -- --list-different",
    "prebuild": "npm run generate:all",
    "validate": "npm-run-all --parallel check-types && lint-staged",
    "prepare": "husky install",
    "generate:clean": "find . -type f -name '*.generated.tsx' -delete",
    "generate:all": "npm run generate:clean && graphql-codegen --config codegen.yml",
    "clear-jest-cache": "jest --clearCache",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.16.0",
    "@chakra-ui/storybook-addon": "^4.0.12",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@faker-js/faker": "^6.3.1",
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/near-operation-file-preset": "^1.13.5",
    "@graphql-codegen/typescript": "^1.13.5",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.13.5",
    "@graphql-codegen/typescript-operations": "^1.13.5",
    "@graphql-codegen/typescript-react-apollo": "^1.13.5",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.1.1",
    "@types/history": "^5.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.176",
    "@types/react-dom": "^18.3.0",
    "@types/react-facebook-login": "^4.1.5",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "copy-webpack-plugin": "^6.4.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "eslint-webpack-plugin": "^4.0.1",
    "factory.ts": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.7",
    "lint-staged": "^11.2.3",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.4.1",
    "sonarqube-scanner": "^2.8.1",
    "ts-jest": "^27.0.7",
    "tsc-files": "^1.1.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
